{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "@vue/airbnb",
    "plugin:prettier-vue/recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "global-require": "off",
    "import/no-dynamic-require": "off"
  }
}
